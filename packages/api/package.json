  "homepage": "https://github.com/bryopsida/sarif-to-xccdf#readme",
  "devDependencies": {
    "jsdoc": "^4.0.2",
    "standard": "^17.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
